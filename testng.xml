<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
	<listeners>
		<listener class-name="mvn.pck.practice5.tests.TestExecutionListeners"/>
	</listeners>
	<test thread-count="5" name="RegisterTest">
		<classes>
			<class name="mvn.pck.practice5.tests.RegisterTest" />
		</classes>
	</test>
	<test thread-count="5" name="LoginTest">
		<classes>
			<class name="mvn.pck.practice5.tests.LoginTest" />
		</classes>
	</test>
</suite>
